  <Experiment name="Voices (HyperNEAT)">
    <AssemblyPath>SharpNeatExperiments.dll</AssemblyPath>
    <ClassName>SharpNeat.Experiments.Classification.Voices.VoicesExperimentHyperNeat</ClassName>
    <Config>
      <PopulationSize>200</PopulationSize>
      <SpecieCount>10</SpecieCount>
      <ActivationCppn>
        <Scheme>Acyclic</Scheme>
      </ActivationCppn>
      <Activation>
